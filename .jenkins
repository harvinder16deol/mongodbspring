pipeline {
    agent any
    tools {
        maven 'Maven 3.8.1'
        jdk 'jdk8'
    }
    stages {
        stage ('Compile Stage') {

            steps {
                    bat 'mvn clean compile'
            }
        }
        stage ('Install Stage') {
            steps {
                    bat 'mvn install'
            }
        }
    }
}
